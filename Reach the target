import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner st = new Scanner(System.in);
		int t = st.nextInt();
		while(t!=0){
		    int x = st.nextInt();
		    int y = st.nextInt();
		    System.out.println(x-y);
		    
		    t--;
		}
	}
}
